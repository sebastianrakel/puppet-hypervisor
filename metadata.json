{
  "name": "puppet-hypervisor",
  "version": "0.1.0",
  "author": "Sebastian Rakel",
  "summary": "installs, configures, and manages a libvirt hypervisor",
  "license": "AGPL-3.0",
  "source": "https://github.com/sebastianrakel/puppet-hypervisor.git",
  "project_page": "https://github.com/sebastianrakel/puppet-hypervisor",
  "issues_url": "https://github.com/sebastianrakel/puppet-hypervisor/issues",
  "description": "This is a nice module to manage libvirt",
  "tags": [
    "libvirt",
    "hypervisor",
    "systemd-networkd"
  ],
  "dependencies": [
    {
      "name": "puppet/systemd",
      "version_requirement": ">= 3.0.0 < 9.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 7.1.0 < 10.0.0"
    },
    {
      "name": "cirrax/libvirt",
      "version_requirement": ">= 6.0.0 < 7.0.0"
    },
    {
      "name": "puppet-nftables",
      "version_requirement": ">= 7.0.0 < 8.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "22.04",
	"24.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "openvox",
      "version_requirement": ">= 8.19.0 < 9.0.0"
    }
  ]
}
